{

 "manifest_version": 2,
 "name": "metasearch", 
 "description": "recherche avancee", 
 "version": "1.0", 
"browser_action" :
	{
		"default_icon" : "icon.png",
		"default_popup" :"popup.html",
    	"default_title": "Test"

},
 "content_scripts": [ {
    "js": [ "jquery.min.js","test.js" ],
    "matches": [ "http://*/*", "https://*/*","http://jquery.com/*"]


  }],


"permissions": [
    "activeTab",
    "<all_urls>"
]


}

